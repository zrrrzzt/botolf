{
  "name": "botolf",
  "description": "my test of the botframework",
  "version": "1.6.0",
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no"
  },
  "license": "MIT",
  "keywords": [
    "bot",
    "chatbot"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/zrrrzzt/botolf.git"
  },
  "bugs": {
    "url": "http://github.com/zrrrzzt/botolf/issues"
  },
  "main": "index.js",
  "engines": {
    "node": ">=6.9.3"
  },
  "scripts": {
    "start": "node index.js",
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "now-deploy": "npm test && now -e APP_ID=@botolf-app-id -e APP_SECRET=@botolf-app-secret && now alias"
  },
  "now": {
    "alias": "botolf.allthethings.win"
  },
  "devDependencies": {
    "coveralls": "3.0.5",
    "nsp": "3.2.1",
    "standard": "13.1.0",
    "tap": "14.6.1"
  },
  "dependencies": {
    "botbuilder": "4.5.3",
    "brreg": "4.0.1",
    "fortune-cookie": "0.0.1",
    "got": "9.6.0",
    "restify": "8.3.3",
    "wreck": "14.2.0"
  }
}
